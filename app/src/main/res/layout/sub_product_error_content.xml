                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textSize="@dimen/font14"
                android:textColor="@color/teal_700"
                android:layout_marginLeft="5dp"
                android:background="@drawable/textview_bg_gray"/>

            <TextView
                android:id="@+id/subDetailErrorSeq1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/detail_content_input_margin"
                android:textColor="@color/list_item_icon"
                android:textSize="@dimen/detail_content_textsize" />

        </LinearLayout>

    </LinearLayout>

</ScrollView>